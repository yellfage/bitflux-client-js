{
  "name": "@yellfage/wst-client-js",
  "version": "1.0.0",
  "author": "Yellfage",
  "description": "WST javascript client",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clear:dist compile:prod create:declarations",
    "compile:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "create:declarations": "tsc --project ./tsconfig.json --outDir './dist' --emitDeclarationOnly",
    "clear:dist": "rimraf -rf ./dist/*",
    "commit": "commit",
    "lint": "eslint ./ --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/prompt-cli": "^12.1.1",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.2.2",
    "commitlint": "^12.1.1",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-standard": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "delay": "^5.0.0",
    "uuid": "^8.3.2"
  }
}
